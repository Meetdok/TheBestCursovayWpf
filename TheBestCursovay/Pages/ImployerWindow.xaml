<Window x:Class="TheBestCursovay.Pages.ImployerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TheBestCursovay.Pages"
        mc:Ignorable="d"
        Title="" Height="450" Width="800">
    <Grid>
        <Menu Height="23" VerticalAlignment="Top">
            <MenuItem Header="Списки поступления" Cursor="Hand" Height="23">
                <MenuItem Header="Список" Foreground="#FF080303" Cursor="Hand" Command="{Binding MainWindow2}"/>
                <MenuItem Header="Список Поступления" Foreground="#FF080303" Cursor="Hand" Command="{Binding LastList}"/>
            </MenuItem>
        </Menu>
        <Frame Content="{Binding CurrentPage}" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
